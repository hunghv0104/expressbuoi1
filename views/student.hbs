<table class="table table-primary">
 <tr class="text-center">
  <th colspan="6">Student list</th>
 </tr>
 <tr class="text text-success">
  <th>No.</th>
  <th>Name</th>
  <th>DoB</th>
  <th>Gender</th>
  <th>Mobile</th>
  <th>Email</th>
 </tr>
 {{#each students }}
 <tr>
  <th></th>
  <th>{{this.name}}</th>
  <th>{{this.dob}}</th>
  <th>{{this.gender}}</th>
  <th>{{ this.mobile }}</th>
  <th>{{ this.email }}</th>
 </tr>
 {{/each}}
</table>